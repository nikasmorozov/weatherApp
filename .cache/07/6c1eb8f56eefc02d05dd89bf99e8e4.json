  color: grey;\n  font-size: 16px;\n}\n\n.locationsCard {\n  display: flex;\n  flex-direction: column;\n  padding: 0;\n  transform: translateY(-500px);\n}\n\n.weatherCard {\n  position: relative;\n  display: flex;\n  flex-direction: column;\n  /* justify-content: center; */\n  padding: 3% 10% 5% 10%;\n  width: 450px;\n  height: 800px;\n  box-sizing: border-box;\n  text-align: center;\n}\n\n.AboveZero {\n  background: #cbba83;\n  border-top: 50px solid #ffecb3;\n}\n\n.BelowZero {\n  background: #81b9bf;\n  border-top: 50px solid #b2ebf2;\n}\n\ninput, button {\n  background: #ffecb3;\n  border: none;\n  color: grey;\n  font-size: 14px;\n  text-transform: uppercase;\n  text-align: center;\n  padding: 5px;\n  box-sizing: border-box;\n}\n\nbutton {\n  text-align: left;\n}\n\ninput {\n  margin-top: 25px;\n  border: 2px solid white;\n}\n\ninput:focus {\n  outline: none;\n  width: 100%;\n}\n\nbutton:focus {\n  background-color: #cbba83;\n  outline: none;\n  color: white;\n}\n\n.conditionSymbol {\n  width: 40px;\n  height: 40px;\n  display: inline-flex;\n  background-position: bottom;\n  background-size: contain;\n  background-repeat: no-repeat;\n  fill: white;\n  margin-left: 20px;\n}\n\n.timeStamp:nth-of-type(1) .conditionSymbol{\n  margin-bottom: 30px;\n  width: 200px;\n  height: 200px;\n}\n\n.clear {\n  background-image: url(./icons/./sunny.svg);\n}\n\n.isolated-clouds {\n  background-image: url(./icons/isolated-clouds.svg);\n}\n\n.scattered-clouds {\n  background-image: url(./icons/scattered-clouds.svg);\n}\n\n.overcast {\n  background-image: url(./icons/overcast.svg);\n}\n\n.light-rain, .moderate-rain, .heavy-rain {\n  background-image: url(./icons/rain.svg);\n}\n\n.light-snow, .moderate-snow, .heavy-snow {\n  background-image: url(./icons/snow.svg);\n}\n\n.sleet {\n  background-image: url(./icons/snow.svg);\n}"},"lineCount":null}},"error":null,"hash":"3516426106989679694fe25a36bdf2e7","cacheData":{"env":{}}}