margin: 0;\n  line-height: 1;\n  letter-spacing: 3px;\n  font-weight: 200;\n  text-transform: uppercase;\n}\n\n.temperature {\n  font-size: 24px;\n  margin-bottom: 10px;\n}\n\n.temperature-big {\n  transform: translateX(30px);\n  font-size: 120px;\n}\n\nh3 {\n  font-size: 48px;\n  margin-top: 30px;\n}\n\n.weatherConditions-big {\n  font-size: 26px;\n}\n\n.utcTime {\n  color: rgb(99, 99, 99);\n  font-size: 14px;\n}\n\n.utcTime-big {\n  color: rgb(99, 99, 99);\n  font-size: 18px;\n  margin-top: 5px;\n  margin-bottom: 20px;\n}\n\ninput, button {\n  /* background: #ffe48a; */\n  border: none;\n  color: rgb(99, 99, 99);\n  font-size: 14px;\n  text-transform: uppercase;\n  text-align: center;\n  padding: 5px;\n  box-sizing: border-box;\n  width: 100%;\n}\n\nbutton {\n  text-align: left;\n}\n\ninput {\n  margin-top: 5px;\n  border: 2px solid white;\n}\n\ninput:focus {\n  outline: none;\n}\n\nbutton:focus {\n  background-color: #d1ac32;\n  outline: none;\n  color: white;\n}\n\n.weatherIcon {\n  width: 40px;\n  height: 40px;\n  display: inline-flex;\n  background-position: bottom;\n  background-size: contain;\n  background-repeat: no-repeat;\n  fill: white;\n  margin-bottom: 10px;\n}\n\n.weatherIcon-big{\n  margin-bottom: 30px;\n  width: 200px;\n  height: 200px;\n}\n\n.clear {\n  background-image: url(./icons/./sunny.svg);\n}\n\n.isolated-clouds {\n  background-image: url(./icons/isolated-clouds.svg);\n}\n\n.scattered-clouds {\n  background-image: url(./icons/scattered-clouds.svg);\n}\n\n.overcast {\n  background-image: url(./icons/overcast.svg);\n}\n\n.light-rain, .moderate-rain, .heavy-rain {\n  background-image: url(./icons/rain.svg);\n}\n\n.light-snow, .moderate-snow, .heavy-snow {\n  background-image: url(./icons/snow.svg);\n}\n\n.sleet {\n  background-image: url(./icons/snow.svg);\n}"},"lineCount":null}},"error":null,"hash":"1f586413c08f24baf3eeae3945143b33","cacheData":{"env":{}}}