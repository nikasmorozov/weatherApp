atureData:nth-of-type(1) {\n  font-size: 2rem;\n}\n\n.conditionSymbol {\n  width: 30px;\n  height: 30px;\n  display: inline-block;\n  background-position: center;\n  background-size: contain;\n  background-repeat: no-repeat;\n  fill: white;\n  margin-left: 20px;\n}\n\n.temperatureData:nth-of-type(1) .conditionSymbol{\n  width: 60px;\n  height: 60px;\n}\n\n.clear {\n  background-image: url(./icons/./sunny.svg);\n}\n\n.isolated-clouds {\n  background-image: url(./icons/isolated-clouds.svg);\n}\n\n.scattered-clouds {\n  background-image: url(./icons/scattered-clouds.svg);\n}\n\n.overcast {\n  background-image: url(./icons/overcast.svg);\n}\n\n.light-rain, .moderate-rain, .heavy-rain {\n  background-image: url(./icons/rain.svg);\n}\n\n.light-snow, .moderate-snow, .heavy-snow {\n  background-image: url(./icons/snow.svg);\n}\n\n.sleet {\n  background-image: url(./icons/snow.svg);\n}"},"lineCount":null}},"error":null,"hash":"c67b0d5cf164460598a7a92e0f728b28","cacheData":{"env":{}}}