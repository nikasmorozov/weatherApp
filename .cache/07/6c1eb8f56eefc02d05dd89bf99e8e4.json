 margin: 0;\n  line-height: 1;\n  letter-spacing: 3px;\n  font-weight: 200;\n  text-transform: uppercase;\n}\n\nh3 {\n  font-size: 48px;\n  margin: 40px 0 5px 0;\n}\n\n.preloader {\n  background-image: url(./preloader/2.gif);\n  width: 64px;\n  height: 64px;\n  position: absolute;\n  top: 50%;\n  left: 50%;\n  transform: translateY(-50%);\n  transform: translateX(-50%);\n  z-index: 2;\n}\n\n.searchGroup {\n  position: relative;\n}\n\n.locationsCard {\n  padding: 0;\n  margin: 0;\n  width: 70%;\n  margin-top: 10px;\n  z-index: 1;\n}\n\n.weatherCard {\n  position: relative;\n  display: flex;\n  flex-direction: column;\n  padding: 3% 15% 5% 15%;\n  justify-content: center;\n  width: 100vw;\n  height: 100vh;\n  box-sizing: border-box;\n  text-align: center;\n}\n\n.AboveZero {\n  background: #d1ac32;\n}\n\n.BelowZero {\n  background: #81b9bf;\n}\n\n.nextHoursWeather {\n  display: flex;\n  justify-content: space-between;\n  margin-top: 50px;\n}\n\n.temperature {\n  font-size: 24px;\n  margin-bottom: 10px;\n  transform: translateX(7px);\n}\n\n.temperature-big {\n  transform: translateX(30px);\n  font-size: 120px;\n}\n\n.weatherConditions-big {\n  font-size: 26px;\n}\n\n.utcTime {\n  color: rgb(99, 99, 99);\n  font-size: 14px;\n}\n\n.utcTime-big {\n  color: rgb(99, 99, 99);\n  font-size: 18px;\n  margin-top: 5px;\n  margin-bottom: 50px;\n}\n\ninput, button {\n  border: none;\n  color: rgb(99, 99, 99);\n  font-size: 14px;\n  text-transform: uppercase;\n  text-align: center;\n  padding: 5px;\n  box-sizing: border-box;\n  width: 100%;\n}\n\nbutton {\n  text-align: left;\n}\n\ninput {\n  border: 2px solid white;\n}\n\ninput::placeholder {\n  letter-spacing: 2px;\n}\n\ninput:focus {\n  outline: none;\n}\n\ninput:focus::-webkit-input-placeholder{\n  color: transparent!important;\n}\ninput:focus::-moz-placeholder{\n  color: transparent!important;\n}\ninput:focus:-moz-placeholder{\n  color: transparent!important;\n}\n\nbutton:focus {\n  outline: none;\n  color: white;\n}\n\n.buttonBackgroundWarm {\n  background-color: #ffe48a;\n}\n\n.buttonBackgroundWarm:focus {\n  background-color: #d1ac32;\n}\n\n.buttonBackgroundCold {\n  background-color: #b2ebf2;\n}\n\n.buttonBackgroundCold:focus {\n  background-color: #81b9bf;\n}\n\n.weatherIcon {\n  width: 40px;\n  height: 40px;\n  display: inline-flex;\n  background-position: bottom;\n  background-size: contain;\n  background-repeat: no-repeat;\n  fill: white;\n  margin-bottom: 10px;\n}\n\n.weatherIcon-big{\n  margin-bottom: 30px;\n  width: 200px;\n  height: 200px;\n}\n\n.clear {\n  background-image: url(./icons/./clear.svg);\n}\n\n.isolated-clouds {\n  background-image: url(./icons/isolated-clouds.svg);\n}\n\n.scattered-clouds {\n  background-image: url(./icons/scattered-clouds.svg);\n}\n\n.overcast {\n  background-image: url(./icons/overcast.svg);\n}\n\n.light-rain, .moderate-rain {\n  background-image: url(./icons/rain.svg);\n}\n\n.heavy-rain {\n  background-image: url(./icons/heavy-rain.svg);\n}\n\n.light-snow, .moderate-snow, .heavy-snow {\n  background-image: url(./icons/snow.svg);\n}\n\n.sleet {\n  background-image: url(./icons/snow.svg);\n}\n\n@media screen and (max-width: 768px){\n  .weatherCard {\n    padding: 3% 7% 3% 7%;\n  }\n\n  .weatherIcon {\n    width: 35px;\n    height: 35px;\n  }\n\n  h3 {\n    margin-top: 20px;\n    font-size: 34px;\n  }\n\n  .weatherIcon-big{\n    margin-bottom: 20px;\n    width: 90px;\n    height: 90px;\n  }\n\n  .temperature-big {\n    transform: translateX(20px);\n    font-size: 80px;\n  }\n\n  .weatherConditions-big {\n    font-size: 20px;\n  }\n\n  .nextHoursWeather {\n    margin-top: 50px;\n    margin-bottom: 60px;\n  }\n\n  .timeStamp-small:nth-of-type(6),\n  .timeStamp-small:nth-of-type(7) {\n  display: none;\n  }\n\n  .utcTime-big {\n    margin-bottom: 20px;\n  }\n\n  .locationsCard {\n    width: 86%;\n  }\n}"},"lineCount":null}},"error":null,"hash":"09144a0c1d00f12a40bc52e97b64ee0f","cacheData":{"env":{}}}